    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "php": "^8.1",
        "psr/http-message":"^1.0.1",
        "psr/http-factory":"^1.0.1",
        "teknoo/immutable": "^3.0"
     },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.6||^10",
        "squizlabs/php_codesniffer": "^3.7.1",
        "sebastian/phpcpd": "^6.0.3",
        "phpstan/phpstan": "^1.9.14",
        "symfony/property-access": "^6.2.5",
        "icanhazstring/composer-unused": "^0.8.5",
        "nikic/php-parser": "^v4.13.2",
        "guzzlehttp/guzzle":"^7.4",
        "guzzlehttp/psr7":"^2.4",
        "php-http/client-common": "^2.5",
        "php-http/multipart-stream-builder": "^1.2.0",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/promise": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Teknoo\\Jenkins\\": "src/",
            "Teknoo\\Jenkins\\Transport\\Guzzle6\\": "infrastructures/Guzzle6",
            "Teknoo\\Jenkins\\Transport\\HttpPlug\\": "infrastructures/HttpPlug"
        }
    }
}
